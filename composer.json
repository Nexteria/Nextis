{
    "name": "nexteria/nextis",
    "description": "Nexteria system.",
    "keywords": ["nexteria", "space"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ddeath/graphql-laravel.git"
        }
    ],
    "require": {
        "barryvdh/laravel-snappy": "0.4.1",
        "laravel/framework": "5.3.*",
        "laravelcollective/html": "5.3.*",
        "zizaco/entrust": "5.2.x-dev",
        "mews/purifier": "^2.0",
        "guzzlehttp/guzzle": "~6.0.0",
        "sentry/sentry-laravel": "^0.9.2",
        "doctrine/dbal": "v2.5.4",
        "maatwebsite/excel": "~2.1.0",
        "google/apiclient": "^2.0",
        "webpatser/laravel-uuid": "2.*",
        "bogardo/mailgun": "^5.0",
        "php-http/guzzle6-adapter": "^1.1",
        "owen-it/laravel-auditing": "^4.0",
        "propaganistas/laravel-phone": "^2.8",
        "propaganistas/laravel-intl": "^1.2",
        "faustbrian/laravel-commentable": "2.1.4",
        "intervention/image": "^2.4",
        "barryvdh/laravel-cors": "^0.11.0",
        "rebing/graphql-laravel": "dev-feature-upload-files",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "h4cc/wkhtmltoimage-amd64": "0.12.x"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "3.1.*",
        "squizlabs/php_codesniffer": "^2.7",
        "symfony/dom-crawler": "3.1.*",
        "laracasts/generators": "^1.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
