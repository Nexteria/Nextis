{
  "name": "nextis",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.0.3",
    "@material-ui/icons": "3.0.1",
    "@sentry/browser": "^4.0.0-beta.12",
    "apollo-cache-inmemory": "^1.2.0",
    "apollo-client": "^2.3.0",
    "apollo-link-http": "^1.5.4",
    "apollo-upload-client": "^8.0.0",
    "chartist": "^0.11.0",
    "classnames": "^2.2.5",
    "date-fns": "^1.29.0",
    "diacritics": "^1.3.0",
    "downloadjs": "^1.4.7",
    "downshift": "^1.31.14",
    "fs": "^0.0.1-security",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "leaflet": "^1.3.4",
    "mjml": "^4.1.2",
    "moment": "^2.22.1",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "perfect-scrollbar": "^1.3.0",
    "react": "^16.3.2",
    "react-apollo": "^2.1.4",
    "react-big-calendar": "^0.19.0",
    "react-bootstrap-sweetalert": "^4.4.1",
    "react-chartist": "^0.13.1",
    "react-datetime": "^2.14.0",
    "react-dom": "^16.3.2",
    "react-google-maps": "^9.4.5",
    "react-jvectormap": "^0.0.3",
    "react-leaflet": "^2.0.1",
    "react-nouislider": "^2.0.1",
    "react-router-dom": "4.3.0-rc.3",
    "react-rte": "^0.16.1",
    "react-scripts": "1.1.4",
    "react-select": "^2.1.0",
    "react-spinkit": "^3.0.0",
    "react-swipeable-views": "^0.12.13",
    "react-table": "^6.8.2",
    "react-tagsinput": "^3.19.0",
    "react-waterfall": "^3.0.7",
    "recompose": "^0.27.0",
    "urijs": "^1.19.1",
    "validator": "^10.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "NODE_PATH=./src && REACT_APP_SPACE_VERSION=$(git log --pretty=\"%h\" -n1 HEAD) && npm-run-all -p watch-css start-js",
    "build": "NODE_PATH=./src && npm run compile-mjml && REACT_APP_SPACE_VERSION=$(git log --pretty=\"%h\" -n1 HEAD) && npm run build-css && react-scripts build",
    "deploy": "REACT_APP_SPACE_VERSION=$(git log --pretty=\"%h\" -n1 HEAD) npm run build && cp -r build/* ../public/ && rm ../public/index.html",
    "test": "react-scripts test --env=jsdom",
    "compile-mjml": "babel-node --presets es2015 scripts/compile-mjml.js",
    "eject": "react-scripts eject",
    "lint": "eslint ./src"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0"
  }
}
